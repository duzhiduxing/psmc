                f_main_nav.find("span").fadeOut("fast").eq(index).fadeOut("fast");
            }
        });
    });
    f_main_con.find("a").click(function(){
        $(this).parent().fadeOut("fast");
        f_main_nav.find("span").fadeOut("fast");
    });

});
</script>
<!-- 我们的优势控件end -->

</body>
